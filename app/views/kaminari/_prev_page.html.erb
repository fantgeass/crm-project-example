<% if current_page.first? %>
  <li class="active">
    <%= link_to raw(t 'views.pagination.previous'), '#', :rel => 'prev', :remote => remote, title: 'Предыдущая' %>
  </li>
<% else %>
  <li>
    <%= link_to raw(t 'views.pagination.previous'), url, :rel => 'prev', :remote => remote, title: 'Предыдущая' %>
  </li>
<% end %>
