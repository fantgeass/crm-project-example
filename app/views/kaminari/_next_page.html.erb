<% if current_page.last? %>
  <li class="active">
    <%= link_to raw(t 'views.pagination.next'), '#', :rel => 'next', :remote => remote, title: 'Следующая' %>
  </li>
<% else %>
  <li>
    <%= link_to raw(t 'views.pagination.next'), url, :rel => 'next', :remote => remote, title: 'Следующая' %>
  </li>
<% end %>
